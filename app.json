{
  "expo": {
    "name": "deliveroo",
    "owner": "ibrahimmowafy",
    "slug": "deliveroo",
    "description": "Deliveroo is an online food ordering by browsing through menus with complete images and descriptions for each restaurant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-d.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon-d.png",
      "resizeMode": "contain",
      "backgroundColor": "#00ccbb"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
